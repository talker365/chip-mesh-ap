# Defination of U-Blox GPS 
ACTION=="add", ATTRS{idVendor}=="1546", ATTRS{idProduct}=="01a6", KERNEL=="ttyACM*", SYMLINK+="vdnGPS", OWNER="root", RUN+="/etc/vdn/bin/gps &"

# Defination of Realtek SDR
ACTION=="add", ATTRS{idVendor}=="0bda", ATTRS{idProduct}=="2838", SYMLINK+="vdnSDR", OWNER="root", RUN+="/etc/vdn/bin/d1090 &"

